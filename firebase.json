{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/basic",
        "destination": "/basic.html"
      },
      {
        "source": "/salient",
        "destination": "/salient.html"
      },
      {
        "source": "/natural-language",
        "destination": "/natural-language.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}